<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <gradient
          android:startColor="#FD404056"
          android:endColor="#222232"
          android:type="radial" />
      <corners
          android:radius="8dp"/>
    </shape>
    <shape android:shape="rectangle" android:thickness="1dp">
      <gradient
          android:startColor="#80636375"
          android:endColor="#803B3B4D"
          android:type="linear" />
    </shape>
  </item>
</selector>